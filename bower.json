{
  "name": "mamadecisao",
  "dependencies": {
    "jquery": "latest",
    "bootstrap": "latest",
    "angular": "1.6.x",
    "angular-bootstrap": "latest",
    "angular-sanitize": "latest",
    "font-awesome": "latest",
    "toastr": "latest",
    "dutilsjs": "latest",
    "ng-file-upload": "latest",
    "jcrop": "latest",
    "caman": "latest",
    "admin-lte": "2.3.x",
    "angular-ui-select": "latest",
    "angular-ui-tree": "2.22.6",
    "angular-chart.js": "^1.1.1"
  },
  "install": {
    "base": "public/assets/vendor",
    "path": {
      "js": "js",
      "css": "css",
      "otf": "fonts",
      "eot": "fonts",
      "svg": "fonts",
      "ttf": "fonts",
      "woff": "fonts",
      "woff2": "fonts"
    },
    "sources": {
      "jquery": {
        "mapping": [
          {
            "bower_components/jquery/dist/jquery.min.js": "../jquery.min.js"
          }
        ]
      },
      "bootstrap": {
        "mapping": [
          {
            "bower_components/bootstrap/dist/js/bootstrap.min.js": "../bootstrap.min.js"
          },
          {
            "bower_components/bootstrap/dist/css/bootstrap.min.css": "../bootstrap.min.css"
          },
          {
            "bower_components/bootstrap/dist/css/bootstrap-theme.min.css": "../bootstrap-theme.min.css"
          }
        ]
      },
      "dutilsjs": {
        "mapping": [
          {
            "bower_components/dutilsjs/src/dutils-common.js": "../dutils-common.js"
          },
          {
            "bower_components/dutilsjs/src/dutils-angular-config.js": "../dutils-angular-config.js"
          },
          {
            "bower_components/dutilsjs/src/dutils-angular-directives.js": "../dutils-angular-directives.js"
          }
        ]
